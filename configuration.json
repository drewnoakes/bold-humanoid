{
  "hardware": {
    "cm730-path": "/dev/ttyUSB0",
    "video-path": "/dev/video0",
    "voice": "english-mb-en1",
    "joystick": {
      "enabled": false,
      "x-amp-max": 15.0,
      "y-amp-max": 15.0,
      "a-amp-max": 15.0
    }
  },
  "world": {
    "field-size-x": 6.0,
    "field-size-y": 4.0,
    "goal-size-x": 0.5,
    "goal-size-y": 1.5,
    "goal-size-z": 0.8,
    "goal-post-diameter": 0.1,
    "goal-area-size-x": 0.6,
    "goal-area-size-y": 2.2,
    "penalty-mark-distance": 0.8,
    "circle-diameter": 1.2,
    "line-width": 0.05,
    "penalty-line-length": 0.1,
    "outer-margin-minimum": 0.7,
    "ball-diameter": 0.067
  },
  "camera": {
    "image-width": 320,
    "image-height": 240,
    "focal-length": 0.025,
    "field-of-view": {
      "vertical-degrees": 46.0,
      "horizontal-degrees": 58.0
    },
    "settings": {
      "brightness": 128,
      "contrast": 32,
      "saturation": 28,
      "gain": 255,
      "exposure": 1000,
      "auto-wb": 0,
      "power-line-frequency": 2,
      "wb-temp-k": 0,
      "sharpness": 191,
      "backlight-compensation": 0,
      "exposure-auto": 3,
      "exposure-absolute": 133,
      "exposure-auto-priority": 0
    }
  },
  "vision": {
    "min-ball-area": 9,
    "pixel-labels": {
      "goal": { "hue": [44, 60], "sat": [158, 236], "val": [124, 222] },
      "ball": { "hue": [248, 23], "sat": [72, 255], "val": [12, 255] },
      "field": { "hue": [72, 118], "sat": [166, 236], "val": [52, 192] },
      "line": { "hue": [0, 255], "sat": [0, 167], "val": [161, 255] }
    }
  },
  "game-controller": {
    "tcp-port": 3838
  },
  "round-table": {
    "tcp-port": 8080,
    "camera-frame-frequency": 5
  },
  "localiser": {
    "particle-count": 100,
    "smoothing-window-size": 5,
    "randomise-ratio": 0.03,
    "reward-fall-off": 0.5,
    "use-lines": true,
    "min-goals-needed": 1,
    "position-error": 0.03,
    "angle-error-degrees": 2
  },
  "head-module": {
    "p-gain": 8,
    "pan-limit-degrees": [-80, 80],
    "tilt-limit-degrees": [-30, 0],
    "tracking": {
      "pan": {
        "p-gain": 0.03,
        "d-gain": 0.1
      },
      "tilt": {
        "p-gain": 0.03,
        "d-gain": 0.1
      }
    }
  },
  "walk-module": {
    "x-offset": -10,
    "y-offset": 5,
    "z-offset": 20,
    "r-offset": 0,
    "p-offset": 0,
    "a-offset": 0,
    "hip-pitch-offset": 17,
    "period-time": 600,
    "dsp-ratio": 0.1,
    "z-move-amplitude": 40,
    "y-swap-amplitude": 20,
    "z-swap-amplitude": 5,
    "pelvis-offset": 3,
    "arm-swing-gain": 1.5,
    "balance-knee-gain": 1.2,
    "balance-ankle-pitch-gain": 3.6,
    "balance-hip-roll-gain": 2,
    "balance-ankle-roll-gain": 4,
    "p-gain": 32,
    "i-gain": 0,
    "d-gain": 0
