  "ambulator": {
    "auto-balance": true,
    "x-amp-delta": 3,
    "y-amp-delta": 3,
    "turn-delta": 1,
    "max-hip-pitch-at-speed": 15,
    "min-hip-pitch": 13,
    "max-hip-pitch": 17
  },
  "options": {
    "circle-ball": {
      "walk-x": 1,
      "walk-y": 20,
      "walk-turn": 15
    },
    "approach-ball": {
      "turn-scale": 17.5,
      "max-forward-speed": 30,
      "min-forward-speed": 5,
      "break-distance": 0.5
    }
  }
}
