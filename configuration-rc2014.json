{
  "inherits": "configuration-team.json",

  "team-number": 3,

  "camera": {
    "settings": {
      "auto-wb": false,
      "exposure-auto": 1,
      "exposure-absolute": 500,
      "exposure-auto-priority": false,
      "backlight-compensation": false,
      "power-line-frequency": 2,
      "brightness": 100,
      "contrast": 32,
      "saturation": 28,
      "gain": 0,
      "wb-temp-k": 6000,
      "sharpness": 0
    }
  },

  "world": {
    "ball-diameter": 0.1
  },

  "vision": {
    "pixel-labels": {
        "ball": { "hue": [0, 15], "sat": [160, 255], "val": [90, 190] },
        "field": { "hue": [60, 100], "sat": [50, 255], "val": [45, 145] },
        "goal": { "hue": [40, 50], "sat": [140, 255], "val": [84, 150] },
        "line": { "hue": [0, 255], "sat": [0, 167], "val": [120, 255] }
    },
    "field-edge-pass": {
      "min-vertical-run-length": 13
    },
    "goal-detection": {
      "max-field-edge-distance-px": 10,
      "accepted-width-ratio": [0.1, 2.0]
    },
    "player-detection": {
      "enable": false
    }

  }
}
