{
  "mitecom": {
    "local-port": 12321,
    "remote-port": 12321,
    "send-period-seconds": 0.1
  },
  "drawbridge": {
    "udp-port": 8074,
    "websocket-port": 8888
  },
  "voice": {
    "enabled": true,
    "name":    1,
    "rate":    130,
    "volume":  140,
    "pitch":   0,
    "range":   80,
    "word-gap-ms": 5,
    "capital-accent-hz": 20
  },
  "eyes": {
    "disabled": false
  },
  "vocaliser": {
    "announce-ball-position": false
  },
  "hardware": {
    "cm730-path": "/dev/ttyUSB0",
    "video-path": "/dev/video0",
    "joystick": {
      "enabled": true,
      "path": "/dev/input/js0",
      "head-speed": 15.0,
      "x-amp-max": 15.0,
      "y-amp-max": 15.0,
      "a-amp-max": 15.0
    },
    "jam-detector": {
      "enabled": true,
      "window-size": 2000,
      "high-threshold": 50,
      "low-threshold": 20
    },
    "offsets": {
      "joint-1": 0,
      "joint-2": 0,
      "joint-3": 0,
      "joint-4": 0,
      "joint-5": 0,
      "joint-6": 0,
      "joint-7": 0,
      "joint-8": 0,
      "joint-9": 0,
      "joint-10": 0,
      "joint-11": 0,
      "joint-12": 0,
      "joint-13": 0,
      "joint-14": 0,
      "joint-15": 0,
      "joint-16": 0,
      "joint-17": 0,
      "joint-18": 0,
      "joint-19": 0,
      "joint-20": 0
    }
  },
  "world": {
    "field-size-x": 9.0,
    "field-size-y": 6.0,
    "goal-size-x": 0.5,
    "goal-size-y": 2.25,
    "goal-size-z": 1.1,
    "goal-post-diameter": 0.1,
    "goal-area-size-x": 0.6,
    "goal-area-size-y": 3.45,
    "penalty-mark-distance": 1.8,
    "circle-diameter": 1.5,
    "circle-segment-count": 18,
    "line-width": 0.05,
    "penalty-line-length": 0.1,
    "outer-margin-minimum": 0.7,
    "ball-diameter": 0.1
  },
  "camera": {
    "image-width": 640,
    "image-height": 480,
    "recording-frames": false,
    "field-of-view": {
      "vertical-degrees": 47.68,
      "horizontal-degrees": 61.59
    },
    "calibration": {
      "tilt-angle-degrees": -40
    },
    "settings": {
      "initialise-order": [
        "auto-wb", "exposure-auto", "exposure-absolute", "exposure-auto-priority",
        "backlight-compensation", "power-line-frequency", "brightness", "contrast",
        "saturation", "gain", "wb-temp-k", "sharpness"
      ],
      "auto-wb": false,
      "exposure-auto": 1,
      "exposure-absolute": 400,
      "exposure-auto-priority": false,
      "backlight-compensation": false,
      "power-line-frequency": 2,
      "brightness": 128,
      "contrast": 32,
      "saturation": 28,
      "gain": 100,
      "wb-temp-k": 0,
      "sharpness": 191
    }
  },
  "vision": {
    "ignore-above-horizon": true,
    "image-granularity": 4,
    "max-granularity": 4,
    "pixel-labels": {
      "goal": { "hue": [30, 60], "sat": [158, 236], "val": [124, 222] },
      "ball": { "hue": [248, 30], "sat": [72, 255], "val": [12, 255] },
      "field": { "hue": [72, 118], "sat": [166, 236], "val": [52, 192] },
      "line": { "hue": [0, 255], "sat": [0, 167], "val": [161, 255] }
    },
    "ball-detection": {
      "min-area-px": 1,
      "accepted-size-ratio": [0.4, 1.5],
      "enable-blob-merging": false,
      "max-field-edge-distance-px": 0
    },
    "goal-detection": {
      "min-dimension-px": 3,
      "max-field-edge-distance-px": 30,
      "accepted-width-ratio": [0.5, 1.4],
      "max-pair-error-dist": 0.5
    },
    "line-detection": {
      "enable": false,
      "line-dots": {
        "hysteresis": 3
      },
      "mask-walk": {
        "delta-r": 3.0,
        "delta-theta-degs": 3.0,
        "min-votes": 10,
        "min-line-length": 30,
        "max-line-gap": 30,
        "max-lines-returned": 5
      }
    },
    "label-counter": {
      "enable": false
    },
    "field-edge-pass": {
      "field-edge-type": 1,
      "min-vertical-run-length": 5,
      "use-convex-hull": true,
      "complete": {
        "smoothing-window-length": 5
      }
    }
  },
  "game-controller": {
    "send-response-messages": true,
    "tcp-port": 3838
  },
  "round-table": {
    "tcp-port": 8080,
    "camera-frame-frequency": 10,
    "image-type": 3,
    "image-features": {
      "blobs": true,
      "line-dots": false,
      "observed-lines": true,
      "expected-lines": false,
      "expected-line-edges": false,
      "horizon": true,
      "field-edge": true,
      "occlusion-edge": true,
      "calibration": false,
      "objects": true
    },
    "image-colours": {
      "line-dot":      { "b": 0, "g": 0, "r": 255 },
      "observed-line": { "b": 255, "g": 80, "r": 80 },
      "expected-line": { "b": 255, "g": 0, "r": 0 },
      "horizon":       { "b": 0, "g": 128, "r": 255 },
      "field-edge":    { "b": 0, "g": 255, "r": 0 },
      "occlusion-edge":{ "b": 80, "g": 80, "r": 80 },
      "calibration":   { "b": 255, "g": 255, "r": 0 }
    },
    "cartoon": {
      "background-colour": { "b": 0, "g": 0, "r": 0 }
    }
  },
  "localiser": {
    "smoothing-window-size": 5,
    "randomise-ratio": 0.03,
    "use-lines": false,
    "min-goals-needed": 1,
    "position-error": 0.03,
    "angle-error-degrees": 2
  },
  "head-module": {
    "p-gain": 8,
    "pan-limit-degrees":  [-135.0, 135.0],
    "tilt-limit-degrees": [-22.0, 40.0],
    "tracking": {
      "pan": {
        "p-gain": 0.5,
        "i-gain": 0.0,
        "i-leak": 0.0,
        "d-gain": 0.0
      },
      "tilt": {
        "p-gain": 0.5,
        "i-gain": 0.0,
        "i-leak": 0.0,
        "d-gain": 0.0
      }
    },
    "home-pan":  0.0,
    "home-tilt": 20.0,
    "move-fine": false
  },
  "walk-engine": {
    "params": {
      "x-offset": -10.0,
      "y-offset": 5.0,
      "z-offset": 20.0,
      "roll-offset": 0.0,
      "pitch-offset": 0.0,
      "yaw-offset": 0.0,
      "hip-pitch-offset": 17.0,
      "period-time": 600.0,
      "dsp-ratio": 0.1,
      "foot-height": 40.0,
      "swing-right-left": 20.0,
      "swing-top-down": 5.0,
      "pelvis-offset": 3.0,
      "arm-swing-gain": 1.5,
      "step-fb-ratio": 0.28
    },
    "balance": {
      "enable": true,
      "knee-gain": 1.2,
      "ankle-pitch-gain": 3.6,
      "hip-roll-gain": 2.0,
      "ankle-roll-gain": 4.0
    },
    "gains": {
      "p-gain": 32,
      "i-gain": 0,
      "d-gain": 0
    }
  },
  "walk-module": {
    "auto-balance": true,
    "x-amp-delta": 0.72,
    "y-amp-delta": 0.72,
    "turn-delta": 0.24,
    "max-hip-pitch-at-speed": 13,
    "min-hip-pitch": 11,
    "max-hip-pitch": 14,
    "fwd-acc-hip-pitch-factor": 2.0,
    "stabilisation-time-ms": 300
  },
  "kick": {
    "max-ball-distance": 0.2
  },
  "orientation-tracker": {
    "technique": 0
  },
  "fall-detector": {
    "window-size": 30
  },
  "options": {
    "announce-fsm-states": false,
    "announce-fsm-transitions": false,
    "announce-rate-wpm": 200,
    "circle-ball": {
      "max-speed-x": 20.0,
      "max-speed-y": 20.0,
      "turn-speed-a": 25.0,
      "time-scaling": 8
    },
    "approach-ball": {
      "turn-speed-scale": 18.0,
      "max-forward-speed": 40.0,
      "min-forward-speed": 9.0,
      "brake-distance": 0.6,
      "stop-distance": 0.15,
      "lower-turn-limit-degs": 1.0,
      "upper-turn-limit-degs": 35.0
    },
    "look-around": {
      "top-angle": 30.0,
      "bottom-angle": -25.0,
      "horiz-duration-upper": 2.1,
      "horiz-duration-lower": 1.0,
      "vert-duration": 0.4,
      "speed-step": 0.01
    },
    "look-at-feet": {
      "head-pan-degs": 0,
      "head-tilt-degs": -67.5
    },
    "look-at-ball": {
      "gain": 0.85,
      "offset-min": 2.0,
      "offset-max": 20.0
    },
    "keep-position": {
      "spacing": 1.4
    },
    "yield": {
      "min-dist": 0.5,
      "max-dist": 1.5
    },
    "perfect-line": {
      "enabled": true
    }
  },
  "health-and-safety": {
    "voltage": {
      "high-threshold": 12.0,
      "low-threshold": 10.5,
      "smoothing-window-size": 200
    },
    "temperature": {
      "high-threshold": 60
    }
  }
}
