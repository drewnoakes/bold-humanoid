{
  "inherits": "configuration-team.json",

  "team-number": 7,

  "camera": {
    "settings": {
      "auto-wb": false,
      "exposure-auto": 1,
      "exposure-absolute": 400,
      "exposure-auto-priority": false,
      "backlight-compensation": false,
      "power-line-frequency": 2,
      "brightness": 128,
      "contrast": 32,
      "saturation": 28,
      "gain": 100,
      "wb-temp-k": 7700,
      "sharpness": 0
    }
  },

  "world": {
    "ball-diameter": 0.1
  },

  "vision": {
    "goal-detection": {
      "accepted-width-ratio": [0.3, 4]
    },

